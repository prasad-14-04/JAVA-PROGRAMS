class Dog 
{
	String name;
	String color;
	int cost;

	public static void main(String[] args) 
	{
		Dog d1=new Dog();
		d1.name="ramu";
		d1.color="red";
		d1.cost=1000;
		System.out.println(d1.name);
		System.out.println(d1.color);
		System.out.println(d1.cost);

		System.out.println("---------------------");

		Dog d2;
		d2=d1;
		d2.name="Puppy";
		d2.color="White";
		d2.cost=999;
		System.out.println(d1.color);
		System.out.println(d1.name);
		System.out.println(d1.cost);

		System.out.println(d2.color);
		System.out.println(d2.name);
		System.out.println(d2.cost);

		
	}
}
